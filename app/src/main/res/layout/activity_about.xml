    <TextView
        android:id="@+id/tv_no3"
        android:layout_width="338dp"
        android:layout_height="20dp"
        android:layout_marginTop="4dp"
        android:text="@string/no3"
        android:fontFamily="@font/trebuc"
        app:layout_constraintEnd_toEndOf="@+id/tv_no2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/tv_no2"
        app:layout_constraintTop_toBottomOf="@+id/tv_no22" />

    <TextView
        android:id="@+id/tv_no2"
        android:layout_width="348dp"
        android:layout_height="21dp"
        android:layout_marginTop="4dp"
        android:text="@string/no2"
        android:fontFamily="@font/trebuc"
        app:layout_constraintEnd_toEndOf="@+id/tv_no11"
