        app:layout_constraintStart_toStartOf="@+id/tv_no11"
        app:layout_constraintTop_toBottomOf="@+id/tv_no11" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="347dp"
        android:layout_height="19dp"
        android:layout_marginTop="4dp"
        android:text="@string/no5"
        android:fontFamily="@font/trebuc"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/tv_no44"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/tv_no44"
        app:layout_constraintTop_toBottomOf="@+id/tv_no44"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="354dp"
        android:layout_height="21dp"
        android:layout_marginTop="4dp"
        android:text="@string/no4"
        android:fontFamily="@font/trebuc"
        app:layout_constraintEnd_toEndOf="@+id/tv_no33"
        app:layout_constraintHorizontal_bias="0.461"
        app:layout_constraintStart_toStartOf="@+id/tv_no33"
        app:layout_constraintTop_toBottomOf="@+id/tv_no33" />

    <TextView
        android:id="@+id/tv_apk_by"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/trebuc"
        android:text="@string/aplikasi_by"
        app:layout_constraintEnd_toEndOf="@+id/tv_versi"
        app:layout_constraintStart_toStartOf="@+id/tv_versi"
        app:layout_constraintTop_toBottomOf="@+id/tv_versi"
        tools:text="@string/aplikasi_by" />

    <TextView
        android:id="@+id/tv_logo_by"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/trebuc"
        android:text="@string/logo_by"
        app:layout_constraintEnd_toEndOf="@+id/tv_apk_by"
        app:layout_constraintStart_toStartOf="@+id/tv_apk_by"
        app:layout_constraintTop_toBottomOf="@+id/tv_apk_by"
        tools:text="@string/logo_by" />

    <TextView
        android:id="@+id/tv_no22"
        android:layout_width="314dp"
        android:layout_height="19dp"
