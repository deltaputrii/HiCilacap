        app:layout_constraintStart_toStartOf="@+id/tv_no3"
        app:layout_constraintTop_toBottomOf="@+id/tv_no3" />

    <TextView
        android:id="@+id/tv_no11"
        android:layout_width="315dp"
        android:layout_height="20dp"
        android:layout_marginTop="4dp"
        android:text="@string/no11"
        android:fontFamily="@font/trebuc"
        app:layout_constraintEnd_toEndOf="@+id/tv_no1"
        app:layout_constraintStart_toStartOf="@+id/tv_no1"
        app:layout_constraintTop_toBottomOf="@+id/tv_no1" />

    <TextView
        android:id="@+id/tv_no55"
        android:layout_width="324dp"
        android:layout_height="19dp"
        android:layout_marginTop="4dp"
        android:text="@string/no55"
        android:fontFamily="@font/trebuc"
        app:layout_constraintEnd_toEndOf="@+id/textView5"
        app:layout_constraintHorizontal_bias="0.739"
        app:layout_constraintStart_toStartOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/tv_no555"
        android:layout_width="312dp"
        android:layout_height="20dp"
        android:layout_marginTop="4dp"
        android:text="@string/no555"
        android:fontFamily="@font/trebuc"
        app:layout_constraintEnd_toEndOf="@+id/tv_no55"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/tv_no55"
        app:layout_constraintTop_toBottomOf="@+id/tv_no55" />

    <TextView
        android:id="@+id/tv_no44"
        android:layout_width="330dp"
        android:layout_height="20dp"
        android:layout_marginTop="4dp"
        android:text="@string/no44"
        android:fontFamily="@font/trebuc"
        app:layout_constraintEnd_toEndOf="@+id/textView6"
        app:layout_constraintHorizontal_bias="0.75"
        app:layout_constraintStart_toStartOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/tv_coba"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/btn_register"
        android:fontFamily="@font/trebuc"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/tv_no555"
        app:layout_constraintHorizontal_bias="0.465"
        app:layout_constraintStart_toStartOf="@+id/tv_no555"
        app:layout_constraintTop_toBottomOf="@+id/tv_no555"
        app:layout_constraintVertical_bias="0.102" />

</androidx.constraintlayout.widget.ConstraintLayout>